{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "2amigos/yii2-chartjs-widget": "~2.0",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "2amigos/yii2-resource-manager-component": "*",
        "2amigos/yii2-switch-widget": "~1.0",
        "amnah/yii2-user": "^5.0",
        "asimlqt/php-google-spreadsheet-client": "3.0.*",
        "borales/yii2-phone-input": "*",
        "bower-asset/bootbox": "~4.3.0",
        "bower-asset/bootstrap": "^3.3",
        "bower-asset/perfect-scrollbar": "*",
        "bower-asset/slick-carousel": "*",
        "bower-asset/sweetalert2": "7.3.0",
        "branchonline/yii2-lightbox": "*",
        "codemix/yii2-localeurls": "*",
        "delocker/yii2-animate.css": "dev-master",
        "dmstr/yii2-adminlte-asset": "2.*@dev",
        "eden/mail": "dev-master",
        "fedemotta/yii2-widget-datatables": "*",
        "golovchanskiy/parse-torg12": "dev-master",
        "google/apiclient": "1.1.7",
        "insolita/yii2-migration-generator": "~2.3",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-mpdf": "@dev",
        "kartik-v/yii2-tree-manager": "@dev",
        "kartik-v/yii2-widget-depdrop": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-typeahead": "*",
        "kartik-v/yii2-widgets": "*",
        "kop/yii2-scroll-pager": "dev-master",
        "ktamas77/firebase-php": "dev-master",
        "light/yii2-swagger": "^1.0",
        "modernkernel/yii2-flag-icon-css": "*",
        "mongosoft/yii2-soap-client": "*",
        "mongosoft/yii2-soap-server": "*",
        "mongosoft/yii2-upload-behavior": "*",
        "nirvana-msu/yii2-showloading": "1.0.*@dev",
        "nizsheanez/yii2-asset-converter": "1.*",
        "npm-asset/core-js": "*",
        "npm-asset/croppie": "*",
        "npm-asset/jquery": "^2.2",
        "npm-asset/readmore-js": "*",
        "ofat/yii2-yashop-ses": "*",
        "oomphinc/composer-installers-extender": "^1.1",
        "phpoffice/phpexcel": "*",
        "rmrevin/yii2-fontawesome": "~2.15",
        "understeam/yii2-fcm": "~0.1",
        "xj/yii2-user-agent": "^1.0.0",
        "yandex-money/yandex-checkout-sdk-php": "*",
        "yii2assets/yii2-fullscreen-modal": "*",
        "yii2assets/yii2-printthis": "^1.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-elasticsearch": "^2.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-dropdown-x": "*",
        "loveorigami/yii2-magnific-popup": "@dev",
        "yii2assets/yii2-pdfjs": ">=1.0",
        "phpoffice/phpspreadsheet": "^1.3",
        "php-amqplib/php-amqplib": "^2.7",
        "vyants/yii2-daemon": "^0.1.0",
        "cboden/ratchet": "^0.4.1",
        "react/zmq": "^0.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
         "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "vendor/bower/{$name}/": ["type:bower-asset"],
            "vendor/npm/{$name}/": ["type:npm-asset"]
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
